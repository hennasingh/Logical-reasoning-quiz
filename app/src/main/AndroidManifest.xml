<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="artist.web.logicalreasoningquiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LogicQuizActivity"></activity>
        <activity android:name=".NumberQuiz"></activity>
        <activity android:name=".VerbalQuiz"></activity>
        <activity android:name=".AnalogyQuiz"></activity>
        <activity android:name=".PictoQuiz"></activity>

    </application>

</manifest>